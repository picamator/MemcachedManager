{
    "name": "picamator/memcachedmanager",
    "description": "Wrapper over memchached to optimise saving API/'s search result'",
    "keywords": ["cache", "memcached", "management", "wrapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergii Pryz",
            "email": "picamator@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
			"Picamator\\MemcachedManager\\": "src/"
        },
       	"exclude-from-classmap": [
            "/dev/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Picamator\\MemcachedManager\\Tests\\Unit\\": "dev/tests/unit/src"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "url": "git@github.com:picamator/CacheManager.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "~3.1",
        "symfony/proxy-manager-bridge": "~3.1",
        "symfony/config": "~3.1",
        "symfony/yaml": "~3.1",
        "cache/memcached-adapter": "~0.3",
        "picamator/CacheManager": "dev-dev"
  	},
   "require-dev": {
        "phpunit/phpunit": "~5.5"
   }
}
